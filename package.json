{
  "name": "sui-design",
  "version": "1.0.33",
  "description": "SX UI Components",
  "private": false,
  "main": "lib/sui.js",
  "scripts": {
    "serve": "webpack-dev-server --progress --config ./config/base.js",
    "build": "cross-env NODE_ENV=production webpack --progress --config ./config/base.js",
    "build:report": "cross-env NODE_ENV=production webpack --progress --config ./config/base.js --env.report",
    "lib": "webpack --progress --config ./config/lib.js",
    "lib:report": "webpack --progress --config ./config/lib.js --env.report",
    "lint": "eslint ./src --fix",
    "publish": "node ./publish.js",
    "fab": "fab run_208 publish_208 -f ./publish.py"
  },
  "keywords": [
    "vue",
    "ui"
  ],
  "author": "yangxiang",
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
    "vue": ">2.0.0",
    "iview": ">3.0.0",
    "element-ui": ">2.0.0",
    "moment": ">2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "autoprefixer": "^9.7.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.12.2",
    "clean-webpack-plugin": "^3.0.0",
    "conventional-changelog-cli": "^2.0.28",
    "cross-env": "^7.0.2",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "moment": "^2.24.0",
    "node-sass": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "prismjs": "^1.17.1",
    "sass-loader": "^8.0.0",
    "scp2": "^0.5.0",
    "terser-webpack-plugin": "^2.2.1",
    "url-loader": "^2.2.0",
    "vue": "^2.6.10",
    "vue-clipboard2": "^0.3.1",
    "vue-loader": "^15.7.2",
    "vue-router": "^3.1.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "@babel/runtime-corejs2": "^7.7.2",
    "element-ui": "^2.12.0",
    "iview": "^3.5.3"
  }
